{
  "expo": {
    "name": "<%= props.projectName %>",
    "slug": "<%= props.projectName %>",
    "version": "1.0.0",
    <% if (props.navigationPackage?.name === 'expo-router') { %>
      "scheme": "<%= props.projectName %>",
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
      "expo-router"
      <% if (props.internalizationPackage?.name === "i18next") { %>
        ,"expo-localization"
      <% } %>
      <% if (props.flags?.eas) { %>
        ,[
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ]
      <% } %>
    ],
      "experiments": {
        "typedRoutes": true
        <% if (props.flags.importAlias) { %>
          ,"tsconfigPaths": true
        <% } %>
      },
    <% } else { %>
      "web": {
        "favicon": "./assets/favicon.png"
      },
      <% if (props.flags.importAlias) { %>
        "experiments": {
          "tsconfigPaths": true
        },
      <% } %> 
      "plugins": [
        <% if (props.internalizationPackage?.name === "i18next") { %>
          "expo-localization"
        <% } %>
        <% if (props.flags?.eas && props.internalizationPackage?.name === "i18next") { %>
        ,
        <% } %>
        <% if (props.flags?.eas) { %>
        [
          "expo-dev-launcher",
          {
            "launchModeExperimental": "most-recent"
          }
        ]
        <% } %>
      ],
    <% } %>
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    <% if (props.stylingPackage?.name === "nativewindui") { %>
    "userInterfaceStyle": "automatic",
     <% } else { %>
     "userInterfaceStyle": "light",
    <% } %>
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}