{
  "name": "create-expo-stack",
  "version": "2.1.21",
  "description": "create-expo-stack CLI",
  "private": false,
  "types": "build/types/types.d.ts",
  "bin": {
    "create-expo-stack": "bin/create-expo-stack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danstepanov/create-expo-stack.git"
  },
  "scripts": {
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles -u 2 -a \"./src/templates/**/*\" ./build/templates",
    "build": "yarn clean-build && yarn compile && yarn copy-templates && yarn lint-templates",
    "bump": "ts-node ./src/utilities/bumpVersion.ts",
    "prepublishOnly": "yarn build",
    "publishPublic": "yarn build && npm publish --access public",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint-templates": "ejslint ./src/templates",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "ejs-lint": "^2.0.0",
    "figlet": "^1.6.0",
    "gluegun": "latest",
    "gradient-string": "^2.0.2"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
