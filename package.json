{
  "name": "create-expo-stack-repository",
  "version": "1.0.0",
  "description": "Root package for the create-expo-stack monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/danstepanov/create-expo-stack.git"
  },
  "license": "MIT",
  "workspaces": [
    "cli",
    "www"
