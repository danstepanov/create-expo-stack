{
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "build/**"],
			"cache": true,
			"env": []
		},
		"topo": {
			"dependsOn": ["^topo"]
		},
		"//#format": {
			"outputs": ["node_modules/.cache/prettiercache"],
			"outputMode": "errors-only"
		},
		"format": {
			"dependsOn": ["^topo"],
			"outputs": ["node_modules/.cache/prettiercache"],
			"outputMode": "errors-only"
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
